{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1",
        "headers": {
          "cache-control": "s-maxage=86400"
        }
      }
    ],
    "cleanUrls": true
  }
  